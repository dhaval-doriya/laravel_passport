{
    "private": true,
    "type": "commonjs",
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix",
        "format2": "prettier --log-level warn --write \"**/*.{js,css,md}\"",
        "format_php": "prettier --log-level warn --write \"resources/**/*.blade.php\"",
        "dev": "vite",
        "build": "vite build",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.22.2",
        "axios": "^1.6.4",
        "husky": "^8.0.3",
        "laravel-vite-plugin": "^1.0.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "vite": "^5.0.0"
    },
    "lint-staged": {
        "*.php": "./vendor/bin/php-cs-fixer fix --config.php_cs"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "./vendor/bin/phpunit"
        }
    },
    "dependencies": {
        "@shufo/prettier-plugin-blade": "^1.14.0"
    }
}
